{
  "name": "questify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npx playwright install",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf dist",
    "hasura": "hasura",
    "hasura:cli": "yarn env-cmd -f questify-hasura/.env yarn hasura --project questify-hasura",
    "hasura:start": "docker-compose -f questify-hasura/docker-compose.yml up",
    "hasura:refresh-metadata": "yarn hasura:cli metadata inconsistency status",
    "hasura:dev": "yarn hasura:start && yarn hasura:cli console",
    "hasura:export": "yarn hasura:cli metadata export && yarn graphql-codegen --config codegen.introspect-localhost.yml",
    "graphql-codegen:cached": "postmon --include questify-hasura/schema.graphql graphql/**/*.graphql -- yarn graphql-codegen"
  },
